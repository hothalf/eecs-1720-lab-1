{
  "name": "Extension Project",
  "version": "1.0",
  "description": "Removes letters from google search to create random artistic line art of them",
  "permissions": [
    "tabs",
  "activeTab",
  "storage",
  "default_popup"],

  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon16.png",
    "default_title": "Word change"
    
  },
  "manifest_version": 2

  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": ["program.js"]
  }],

  "background": {
    "scripts": ["background.js"]
  },


}


}

